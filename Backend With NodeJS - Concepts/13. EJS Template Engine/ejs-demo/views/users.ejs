<!DOCTYPE html>
<html>
<head>
  <title>User List</title>
</head>
<body>
  <h1>All Users</h1>

  <% if (users.length > 0) { %>
    <ul>
      <% users.forEach(user => { %>
        <li><%= user.name %> - <%= user.email %></li>
      <% }); %>
    </ul>
  <% } else { %>
    <p>No users found.</p>
  <% } %>
</body>
</html>
